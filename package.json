{
  "name": "qcloud-web",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "http://git.code.oa.com/CFETeam/qcloud-framework.git"
  },
  "main": "index.tsx",
  "scripts": {
    "init": "tnpm install && typings install",
    "dev": "set NODE_ENV=development && node ./develop/index",
    "exp": "set NODE_ENV=development && webpack --config webpack/webpack.exp.js",
    "test": "set NODE_ENV=test && webpack --config webpack/webpack.test.js",
    "build": "set NODE_ENV=production && webpack --config webpack/webpack.prod.js",
    "profile": "set NODE_ENV=production && webpack --config webpack/webpack.prod.js --display-modules --profile --colors",
    "stat": "set NODE_ENV=production && webpack --config webpack/webpack.prod.js --display-modules --profile --json > stat.json",
    "nodeVersion": "node --version",
    "sloc": "sloc ./modules/ --format cli-table -a tsx=ts"
  },
  "dependencies": {
    "@tencent/qcloud-component": "latest",
    "@tencent/qcloud-lib": "latest",
    "@tencent/qcloud-nmc": "latest",
    "@tencent/qcloud-redux-fetcher": "latest",
    "@tencent/qcloud-redux-query": "latest",
    "@tencent/qcloud-redux-workflow": "latest",
    "babel-plugin-transform-es3-member-expression-literals": "^6.3.13",
    "babel-plugin-transform-es3-property-literals": "^6.3.13",
    "babel-polyfill": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "classnames": "^2.2.1",
    "es6-promise": "^3.0.2",
    "flux": "^2.1.1",
    "jframes": "^1.0.0",
    "jquery": "^2.2.1",
    "react": "^0.14.3",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dom": "^0.14.3",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "ts-node": "^0.5.5",
    "tslint-loader": "^2.1.0",
    "typescript": "^1.8.0"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^0.15.9",
    "babel": "^6.5.2",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.7.2",
    "express": "^4.13.4",
    "marked": "^0.3.5",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.2",
    "serve-index": "^1.7.3",
    "ts-loader": "^0.8.2",
    "typescript-require": "^0.2.9-1",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.6.4"
  }
}
